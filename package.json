    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "0.2.3",
    "karma-jasmine": "1.1.0",
    "karma-jasmine-html-reporter": "0.2.2",
    "npm-run-all": "4.0.2",
    "protractor": "5.1.1",
    "rimraf": "2.6.1",
    "stylelint": "7.8.0",
    "ts-node": "2.0.0",
    "tslint": "4.5.1",
    "tslint-eslint-rules": "3.4.0",
    "tslint-language-service": "0.9.3",
    "typescript": "2.3.2"
  }
