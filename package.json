    "bootstrap": "4.0.0-alpha.6",
    "chart.js": "2.5.0",
    "ckeditor": "4.6.2",
    "classlist.js": "1.1.20150312",
    "core-js": "2.4.1",
    "d3": "4.8.0",
    "echarts": "3.5.4",
    "font-awesome": "4.7.0",
    "immutable": "3.8.1",
    "intl": "1.2.5",
